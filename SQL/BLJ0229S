/* BLJ0229S */

DROP TABLE DH_SEDW.D029_TAXA_COMIS_SERVICO;
.IF ERRORCODE <> 0 THEN .GOTO CRIA_STG;

.LABEL CRIA_STG;
CREATE TABLE DH_SEDW.D029_TAXA_COMIS_SERVICO
(
  DATA_PROCESSAMENTO      DATE FORMAT 'YYYYMMDD'
 ,COD_TIPO_SERVICO        CHAR(03)
 ,COD_AGENCIA_OPER_TEL    CHAR(06)
 ,IND_TIPO_COMISS         CHAR(01)
 ,IND_BASE_CALC           CHAR(01)
 ,VAL_BASE_CALC           CHAR(05)

) PRIMARY INDEX(COD_TIPO_SERVICO, COD_AGENCIA_OPER_TEL);
.EXIT ERRORCODE;
